{
  "name": "dev-ops-url",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --forceExit",
    "coverage": "jest --coverage --forceExit",
    "deploycluster": "pm2 start -i 0 src/index.js",
    "stopcluster": "pm2 stop all && pm2 delete all",
    "createdb": "NODE_ENV=dev sequelize db:create && NODE_ENV=test sequelize db:create",
    "migratedb": "NODE_ENV=dev sequelize db:migrate && NODE_ENV=test sequelize db:migrate"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/routes/index.js",
      "!models/*"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "jest": "^22.1.4",
    "nodemon": "^1.15.1",
    "pg": "^7.4.1",
    "pm2": "^2.10.1",
    "sequelize-cli": "^3.2.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "good": "^7.3.0",
    "good-console": "^6.4.1",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.6.2",
    "md5": "^2.2.1",
    "redis": "^2.8.0",
    "sequelize": "^4.34.1"
  }
